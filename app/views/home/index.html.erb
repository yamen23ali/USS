<div class="container">

    <div class="col-lg-8 col-lg-offset-2 search_bar">
       <%= render 'search_bar' %>
    </div>
    <div class="col-lg-2">
    </div>

    <div id="items" class="row list-group">
      <% @assets.each do |asset| @asset = asset %>
        <%= render "grid_cell" %>
      <% end %> 
    </div>

    <div class="col-lg-4 col-lg-offset-4">
        <%= paginate @assets %>
    </div>
</div>
