<h1>Received offers</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>From</th>
      <th>Status</th>
      <th>Creation Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offers.each do |offer| %>
      <tr>
        <td><%= offer.id %></td>
        <td><%= offer.from_id %></td>
        <td><%= offer.status_id %></td>
        <td><%= offer.created_at %></td>
        <td><%= link_to 'Show', show_received_offer_path(offer) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
