<div class="fileinput fileinput-new" data-provides="fileinput">

  <div class = "col-lg-11">
    
    <div class="fileinput-preview thumbnail" data-trigger="fileinput" style="background-color: transparent">
      <%= existing_photos( photo_index, f ) %>
    </div>
  </div>
  
  <div class = "col-lg-1">
      <a href="#" class="btn btn-default fileinput-exists" data-dismiss="fileinput">Remove</a>
  </div>
  
  <div>
       <%= input_element %>
  </div>

</div>